<section class="">
    <section class="container text-center">
        <div class="row">
            <div class="col">
                <h1>The Tech Blog</h1>
            </div>
        </div>
            {{#each posts}}
            <div class="card">
                <h5 class="card-header row mx-0">
                    <p class="col">{{ title }}</p>
                    <p class="col">{{ date_created }}</p>
                </h5>
                <div class="card-body">
                    <p class="card-text">{{ content }}</p>
                </div>
            </div>
            {{/each}}
        </div>
    </section>
</section>

</section>